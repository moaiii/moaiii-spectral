{
  "name": "moai-interactive",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ENV=development browserify -t envify public/js/modules/init.js -o public/js/bundle.js",
    "build:production": "ENV=production browserify -t envify -t uglifyify public/js/modules/init.js -o dist/js/bundle.js; gulp production",
    "start": "concurrently  \"npm run start:styles\" \"npm run start:js\" \"npm run start:server\"",
    "start:server": "nodemon ./bin/www",
    "start:js": "npm run watch",
    "start:styles": "gulp",
    "start:production": "npm run build:production; npm run start:server",
    "watch": "watchify public/js/modules/init.js -o public/js/bundle.js --debug --verbose"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "body-parser": "~1.15.2",
    "browserify": "^14.5.0",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "cross-spawn": "^5.1.0",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "ejs": "~2.5.2",
    "envify": "^4.1.0",
    "express": "~4.14.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.0.0",
    "gulp-express": "^0.3.5",
    "gulp-jshint": "^2.0.4",
    "gulp-jsvalidate": "^2.1.0",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.11",
    "jshint": "^2.9.4",
    "morgan": "~1.7.0",
    "nodemon": "^1.14.3",
    "postcss-scss": "^0.4.0",
    "serve-favicon": "~2.3.0",
    "uglifyify": "^4.0.5",
    "watchify": "^3.9.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "gulp": "^3.9.1"
  }
}
